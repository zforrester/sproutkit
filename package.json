    },
    "devDependencies": {
        "eslint-config-custom": "*",
        "prettier": "^2.7.1",
        "turbo": "latest"
    },
    "engines": {
        "npm": ">=7.0.0",
        "node": ">=14.0.0"
    },
    "packageManager": "npm@8.1.2"
