{
    "name": "sproutkit",
    "description": "UI & Token Library",
    "author": {
        "name": "Zach Forrester",
        "email": "zach.forrester@gmail.com",
        "url": "https://www.zachforrester.me"
    },
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
